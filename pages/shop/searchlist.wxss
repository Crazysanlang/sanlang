page{background-color:#F2F2F2;}
.lista{height:100px;background:#ffffff;margin-top:10px;position:relative;padding:0 5%;display:flex;display: -webkit-flex}
.lista image{width:35px;height:30px;}
.lista .zy{position:absolute;left:0;top:0;}
.lista .vl{flex:2;}
.lista .vl .a{height:30px;padding-top:15px;}
.lista .vl .a .ha{font-size:14px;color:#14bab4;}
.lista .vl .a .hb{font-size:12px;padding-left:12px;}
.lista .vl .a .hc{color:#6a6a6a;font-size:12px;padding-left:6px;padding-right:6px;}
.lista .vl .a .hd{color:#FF7F00;font-size:10px;border:1px solid #FF7F00;border-radius:3px;padding:0 2px;height:10px;line-height:10px;}
.lista .vl .b{display:flex;height:30px;}
.lista .vl .b .cqm{height:14px;}
.lista .vl .b .cqm .m{color:#9a9a9a;padding-right:5px;padding-left:5px;font-size:14px;}
.lista .vl .b .cqm:first-child .m{padding-left:0;}
.lista .vl .b .cqm .n{font-size:16px;padding-right:5px;}
.lista .vl .b .sg{color:#dddddd;padding-top:2px;}
.lista .vl .c{color:#9a9a9a;font-size:12px;}
.lista .vl .c .m{}
.lista .vl .c .n{}
.lista .vl .c image{width:14px;height:14px;position:relative;top:2px;padding-right:3px;}
.lista .vr{text-align:right;float:right}
.lista .vr .a{font-size:12px;color:#a2a2a2;padding-top:17px;height:15px;}
.lista .vr .b{color:#FF7F00;font-size:24px;height:26px;}
.lista .vr .c{font-size:10px;color:#9a9a9a;height:14px;}
.lista .vr .d{font-size:14px;}
.lista .vr .d text{font-size:10px;color:#9a9a9a;padding-right:5px;}
