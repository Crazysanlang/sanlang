.search{display:flex;padding:10px 10px;height:30px;}
.search .l{flex:2}
.search .l .inputbg{border:1px solid #EBEBEB;background-color:#ffffff;border-radius:5px;height:28px;}
.search .l .inputbg image{width:16px;height:16px;float:left;position:relative;top:6px;left:7px;}
.search .l input{font-size:12px;line-height:28px;color:#999999;padding-left:15px;}
page{background-color:#f7f7f7;}
.scrollbox{background-color:#ffffff;margin:5px 10px 0 10px;width:auto;border-radius:5px;}
.list{border-bottom:1px solid #eeeeee;padding:10px 10px 10px 10px;}
.list .bt{font-size:14px;line-height:20px;}
.list .nr{color:#999999;font-size:12px;}
.zwls{text-align:center;font-size:12px;color:#999999;line-height:80px;}