/* 竞价筛选条件样式 */
.filtrate{position:relative}
.filtrate .nav{
    position: relative;
    z-index: 99;
    display: flex;
    height: 30px;
    border-bottom: 1px solid #d5d5d5;
    background: #fff;
}
.filtrate .nav .navTable{
    flex: 1;
    display: flex;
    height: 30px;
    align-items:center;
    justify-content:center;
    font-size: 12px;
    border-right: 1px solid #d5d5d5;
}
.nav>view:last-child{
  border-right:none;
}
.navTable .navTxt{
    display: inline-block;
    color: #333;
}
/* 文本选中样式 */
.navTable.active .navTxt{
    color: #eb344f;
}
.navTable .arrows{
    display: inline-block;
    border: 4px solid transparent;
    border-top: 4px solid #333333;
    margin-left: 5px;
}
/* 箭头选中样式 */
.navTable.active .arrows{
    border-bottom: 4px solid #eb344f;
    border-top:0;
}
/* 遮罩层样式 */
.shade{
    position: fixed;
    top:0;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgb(1, 1, 1);
    transition: all .3s;
    opacity: 0.5;
    display: none;
}
/* 显示遮罩层 */
.showShade{
  display:block;
}
/* 定义下拉菜单动画 */
/* 显示 */
@keyframes slideDown{
    from{
        transform:  translateY(-100%);
    }
    to{
        transform:  translateY(0%);
    }
}
.showCont{
    display: block;
    animation: slideDown .2s ease-in both;
}
/* 隐藏 */
@keyframes slidup{
    from{
        transform:  translateY(0%);
    }
    to{
        transform:  translateY(-100%);
    }
}
.hideCont{
    display: none;
    animation: slidup .2s ease-in both;
}

/* 筛选内容样式 */
.filtrate .navCont{
  position: absolute;
  z-index: 2;
  width: 100%;
  height: 200px;
  text-align: center;
  border: 1px solid red;
  color: #333;
  background-color:#fff; 
}






















