
.weui_cell .em-on,.em-off{position:absolute;right:2%;font-style:normal;font-size:14px;top:0;line-height:45px;height:45px;}
.weui_cell .em-on::after,.em-off::after{border:0;}
.weui_cell .em-on{color:#14bab4;background:#ffffff;}
.weui_cell .em-on[disabled]{background:#ffffff}
.em-on.button-hover{color:#0F9D98}
.weui_cell .em-off{color:#ccc}
.weui_cells_tips {
    margin-top: 1.3em;
    color: #888;
    padding-left: 15px;
    padding-right: 15px;
    font-size: 12px;
    text-align: center;
}
.weui_grids {
    position: relative;
    overflow: hidden;
    font-size:12px;
}
.weui_label {
    display: block;
    width: 105px;
    word-wrap: break-word;
    word-break: break-all;
    
}
.weui_input {
    width: 100%;
    border: 0;
    outline: 0;
    -webkit-appearance: none;
    background-color: transparent;
    color: inherit;
    height:45px;
    font-size:16px;
}
.weui_cell {
    border-bottom:1px solid #ddd;
    padding: 0 5%;
    position: relative;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    background:#ffffff;
    
}
.weui_cella{flex:2}
.weui_cellb{flex:7}
.weui_label {
    display: block;
    width: 150rpx;
    word-wrap: break-word;
    word-break: break-all;
    font-size:12px;
    color:#999999;
}
.weui_btn_area{padding:50rpx 5% 1px;}
page{background:#e9eaec;height:100%}
.tc_modal{position: fixed;background-color: #ffffff;bottom:0;height:300px;width:100%;}