.title{
  height: 90rpx;
  line-height: 90rpx;
  color: #333;
  font-size: 30rpx;
  padding-left: 40rpx;
}
.lx{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 40rpx;
  margin-bottom: 20rpx;
}
.lx view{
  color: #14bab4;
  font-size: 32rpx;
}
.arrow{
  width: 8px;
  height: 8px;
  border-top: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
}

.priceEnter{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-bottom: 1px solid #f5f5f5;
}
.priceEnter view:first-child{
  font-size: 30rpx;
  color: #333;
}
.priceEnter view:last-child{
  width: 66%;
  text-align: left;
}


.botBtn{
  margin: 0 40rpx;
  background-color: #14bab4;
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  border-radius: 3px;
  margin-top: 60rpx;
}