.arrow{
  width: 8px;
  height: 8px;
  border-bottom: 1px solid #999;
  border-right: 1px solid #999;
  transform: rotate(45deg);
}
.comView{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 23rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom:1px solid #f5f5f5
}
.comView .left{
  font-size: 32rpx;
  color: #333;
}
.comView .mid{
  font-size: 30rpx;
  color: #999;
}
.comView .txtInput{
  width: 64%;
  text-align: left;
}
.topView{
  margin: 20rpx 30rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  box-shadow: 1px 1px 10px #999;
}
.attribute{
  margin:0 30rpx;
  padding: 0 23rpx;
  background-color: #fff;
  box-shadow: 1px 1px 10px #999;
}


.botList{
  margin:50rpx;
  /* margin-top:  */
}
.botList .botTit{
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  color: #14bab4;
  background-color: #f5f5f5;
}
.list{
  padding:20rpx;
  border-radius: 3px;
  margin-bottom: 65rpx;
  background-color: #fff;  
  
}
.list.cur{
  /* border: 1px solid #14bab4; */
  box-shadow: 0px 0px 5px #14bab4;
}
.listTop,.listCont{
  display: flex;
  justify-content: space-between;
  height: 30px;
  line-height: 30px;
}
.listTop view{
  font-size: 28rpx;
  color: #333;
}
.listCont view{
  font-size: 22rpx;
  color: #999;
}
.listCont view text{
  font-size: 24rpx;
  color: #333;
}

.botBtn{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  background-color: #14bab4;
  position: fixed;
  bottom: 0;
}

page{
  padding-bottom: 100rpx;
}